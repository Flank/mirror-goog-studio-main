<variant
    name="release"
    package="com.example.app"
    minSdkVersion="14"
    targetSdkVersion="14"
    mergedManifest="build/intermediates/merged_manifest/release/AndroidManifest.xml"
    manifestMergeReport="build/outputs/logs/manifest-merger-release-report.txt"
    proguardFiles="build/intermediates/default_proguard_files/global/proguard-android.txt-$androidGradlePluginVersion"
    partialResultsDir="build/intermediates/lint_partial_results/release/out">
  <buildFeatures
      namespacing="REQUIRED"/>
  <sourceProviders>
    <sourceProvider
        manifests="src/main/AndroidManifest.xml"
        javaDirectories="src/main/java:src/release/java:build/generated/source/buildConfig/release:build/generated/aidl_source_output_dir/release/out:src/main/kotlin:src/release/kotlin"
        resDirectories="build/generated/res/resValues/release:src/release/res:src/main/res"
        assetsDirectories="src/release/assets:src/main/assets"/>
  </sourceProviders>
  <testSourceProviders>
    <sourceProvider
        manifests="src/test/AndroidManifest.xml"
        javaDirectories="src/test/java:src/testRelease/java:src/test/kotlin:src/testRelease/kotlin"
        resDirectories="src/testRelease/res:src/test/res"
        assetsDirectories="src/testRelease/assets:src/test/assets"
        unitTest="true"/>
    <sourceProvider
        manifests="src/androidTest/AndroidManifest.xml"
        javaDirectories="src/androidTest/java:src/androidTestRelease/java:build/generated/source/buildConfig/androidTest/release:build/generated/aidl_source_output_dir/releaseAndroidTest/out:src/androidTest/kotlin:src/androidTestRelease/kotlin"
        resDirectories="build/generated/res/resValues/androidTest/release:src/androidTestRelease/res:src/androidTest/res"
        assetsDirectories="src/androidTestRelease/assets:src/androidTest/assets"
        androidTest="true"/>
  </testSourceProviders>
  <testFixturesSourceProviders>
    <sourceProvider
        manifests="src/testFixtures/AndroidManifest.xml"
        javaDirectories="src/testFixtures/java:src/testFixturesRelease/java:src/testFixtures/kotlin:src/testFixturesRelease/kotlin"
        resDirectories="src/testFixturesRelease/res:src/testFixtures/res"
        assetsDirectories="src/testFixturesRelease/assets:src/testFixtures/assets"/>
  </testFixturesSourceProviders>
  <mainArtifact
      classOutputs="build/intermediates/javac/release/classes:build/intermediates/compile_and_runtime_not_namespaced_r_class_jar/release/R.jar"
      applicationId="com.example.app"
      generatedSourceFolders="build/generated/ap_generated_sources/release/out:build/generated/aidl_source_output_dir/release/out:build/generated/source/buildConfig/release"
      generatedResourceFolders="build/generated/res/resValues/release"
      desugaredMethodsFiles="${gradleCacheDir}/<digest>/transformed/D8BackportedDesugaredMethods.txt">
  </mainArtifact>
  <androidTestArtifact
      applicationId="com.example.app.test"
      generatedResourceFolders="build/generated/res/resValues/androidTest/release"
      desugaredMethodsFiles="${gradleCacheDir}/<digest>/transformed/D8BackportedDesugaredMethods.txt">
  </androidTestArtifact>
  <testFixturesArtifact
      classOutputs="build/intermediates/javac/releaseTestFixtures/classes"
      applicationId="com.example.app.testFixtures"
      generatedSourceFolders="build/generated/ap_generated_sources/releaseTestFixtures/out"
      desugaredMethodsFiles="${gradleCacheDir}/<digest>/transformed/D8BackportedDesugaredMethods.txt">
  </testFixturesArtifact>
  <testArtifact>
  </testArtifact>
</variant>
