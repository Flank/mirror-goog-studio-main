<lint-module
    format="1"
    dir="${projectDir}/app"
    name=":app"
    type="APP"
    maven="lintKotlin:app:"
    gradle="$androidGradlePluginVersion"
    buildFolder="build"
    bootClassPath="${androidSdkDir}/platforms/android-31/android.jar:${androidSdkDir}/build-tools/30.0.3/core-lambda-stubs.jar"
    javaSourceLevel="1.8"
    compileTarget="android-31"
    neverShrinking="true">
  <lintOptions
      disable="GradleDependency"
      abortOnError="true"
      absolutePaths="true"
      checkReleaseBuilds="true"
      explainIssues="true">
    <severities>
      <severity
        id="GradleDependency"
        severity="IGNORE" />
      <severity
        id="KotlinPropertyAccess"
        severity="ERROR" />
      <severity
        id="SyntheticAccessor"
        severity="ERROR" />
    </severities>
  </lintOptions>
  <variant name="debug"/>
</lint-module>
